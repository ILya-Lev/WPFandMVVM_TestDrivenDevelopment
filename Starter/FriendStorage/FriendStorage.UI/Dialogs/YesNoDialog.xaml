<Window x:Class="FriendStorage.UI.Dialogs.YesNoDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FriendStorage.UI.Dialogs"
        mc:Ignorable="d"
        Title="YesNoDialog" Height="159.533" Width="300">
    <Grid RenderTransformOrigin="0.504,0.327">
        <Grid.RowDefinitions>
            <RowDefinition Height="141*"/>
            <RowDefinition Height="131*"/>
        </Grid.RowDefinitions>
        <TextBlock x:Name="_textBlock" TextWrapping="Wrap" Text="TextBlock" TextAlignment="Center" HorizontalAlignment="Center" VerticalAlignment="Center"/>
        <Button x:Name="_btnYes" Content="Yes" HorizontalAlignment="Left" Margin="40,21,0,0" Grid.Row="1" VerticalAlignment="Top" Click="OnClickYesButton"/>
        <Button x:Name="_btnNo" Content="No" HorizontalAlignment="Left" Margin="178,21,0,0" Grid.Row="1" VerticalAlignment="Top" Width="76" Click="OnClickNoButton"/>

    </Grid>
</Window>
